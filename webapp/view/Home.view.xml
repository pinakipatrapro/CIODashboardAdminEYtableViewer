<mvc:View controllerName="pinaki.ey.tableviewer.controller.Home" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	displayBlock="true" xmlns="sap.m">
	<Page title="{i18n>title}">
		<content>
			<VBox alignItems="Center" class="sapUiLargeMargin">
				<items>
					<Input id="idTableNameInput" width="30rem" placeholder="schema.tableName" showSuggestion="true" suggestionItems="{path:'/tableValueHelp'}">
						<core:Item text="{NAME}"/>
					</Input>
					<Button press="onPress" text="Get Details"></Button>
				</items>
			</VBox>
		</content>
	</Page>
</mvc:View>